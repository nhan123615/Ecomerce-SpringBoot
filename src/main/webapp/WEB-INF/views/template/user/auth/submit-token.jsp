<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<jsp:include page="../components/head.jsp"></jsp:include>

<body>
<jsp:include page="../components/header.jsp"></jsp:include>
<jsp:include page="../components/sidebar.jsp"></jsp:include>

<div class="ps-page--my-account">
    <div class="ps-breadcrumb">
        <div class="container">
<%--            <jsp:include page="../message/message.jsp"></jsp:include>--%>
            <ul class="breadcrumb">
                <li><a href="index.html">Home</a></li>
                <li>My account</li>
            </ul>
        </div>
    </div>
    <div class="ps-my-account">
        <div class="container">

            <form class="ps-form--account ps-tab-root" action="${pageContext.servletContext.contextPath}/forgot-password/submit-token" method="post" modelAttribute="token">
                <ul class="ps-tab-list">
                    <li class="active"><a href="#sign-in">Reset Password code</a></li>
                </ul>
                <div class="ps-tabs">
                    <div class="ps-tab active" id="sign-in">
                        <div class="ps-form__content">
                            <h5>Submit Your Reset Password code</h5>
                            <div class="form-group">
                                <input class="form-control" type="text" placeholder="Your Reset Password code" name="resetToken">
                            </div>

                            <div class="form-group submtit">
                                <button class="ps-btn ps-btn--fullwidth">Submit</button>
                            </div>
                        </div>
                        <div class="ps-form__footer">
                        </div>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>
<jsp:include page="../components/footer.jsp"></jsp:include>
</body>

